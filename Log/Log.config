# Log.config
# CRITICAL > ERROR > WARNING > INFO > DEBUG > NOTSET
[loggers]
keys=root, printLogger, recordLogger

[logger_root]
level=DEBUG
handlers=printHandler, recordHandler

[logger_printLogger]
handlers=printHandler
qualname=print
propagate=0

[logger_recordLogger]
handlers=recordHandler
qualname=record
propagate=0

###############################################

[handlers]
keys=printHandler, recordHandler

[handler_printHandler]
class=StreamHandler
level=INFO
formatter=form02
args=(sys.stdout, )

[handler_recordHandler]
class=handlers.RotatingFileHandler
level=DEBUG
formatter=form01
args=('AllLog.log', 'a' ,10*1024*1024,5)
##############################################

[formatters]
keys=form01, form02

[formatter_form01]
format=%(asctime)s %(filename)s %(levelname)s  %(message)s
datefmt=%Y-%m-%d %H: %M: %S

[formatter_form02]
format=%(asctime)s %(filename)s %(levelname)s  %(message)s
datefmt=%Y-%m-%d %H: %M: %S